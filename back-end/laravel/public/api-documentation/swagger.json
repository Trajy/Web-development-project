{
    "openapi": "3.0.0",
    "info": {
        "title": "Employment Api Documentacao",
        "version": "1.0.0"
    },
    "paths": {
        "/api/auth/login": {
            "post": {
                "tags": [
                    "AuthController"
                ],
                "summary": "Realizar o login",
                "description": "end-point obter um token de autorizacao para o usuario",
                "operationId": "8dcb70df1020986038d098cc08d05dae",
                "requestBody": {
                    "content": {
                        "application/json": {
                            "schema": {
                                "required": [
                                    "email",
                                    "password"
                                ],
                                "properties": {
                                    "email": {
                                        "type": "string",
                                        "example": "teste_usuario@example.org"
                                    },
                                    "password": {
                                        "type": "string",
                                        "example": "1234"
                                    }
                                },
                                "type": "object"
                            }
                        }
                    }
                },
                "responses": {
                    "200": {
                        "description": "Retorna o Token e o tipo do usuario",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "properties": {
                                        "token": {
                                            "type": "string",
                                            "example": "3|4pHV2Eb6zqJs3z6MWqPKmKoF7EUONp4MuJCfJYAt"
                                        },
                                        "type": {
                                            "type": "string",
                                            "example": "employer"
                                        }
                                    },
                                    "type": "object"
                                }
                            }
                        }
                    },
                    "401": {
                        "description": "Retorna Invalid Credentials"
                    }
                }
            }
        },
        "/api/auth/logout": {
            "delete": {
                "tags": [
                    "AuthController"
                ],
                "summary": "Realizar o logout",
                "description": "end-point para revogar um token de autorizacao",
                "operationId": "1ef69131b914976d7b0827b2b9d192df",
                "parameters": [
                    {
                        "name": "Authorization",
                        "in": "header",
                        "description": "Baerer token que sera revogado"
                    }
                ],
                "responses": {
                    "200": {
                        "description": "Retorna o Token e o tipo do usuario"
                    },
                    "401": {
                        "description": "Retorno caso o token nao esteja registrado",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "properties": {
                                        "message": {
                                            "type": "string",
                                            "example": "Unauthenticated."
                                        }
                                    },
                                    "type": "object"
                                }
                            }
                        }
                    }
                },
                "security": [
                    {
                        "bearerToken": []
                    }
                ]
            }
        },
        "/api/auth/user": {
            "get": {
                "tags": [
                    "AuthController"
                ],
                "summary": "Obtem os dados do usuario",
                "description": "end-point obter os dados do usuario logado a partir do token",
                "operationId": "e23e5f9b32cba7953a224624a2a16544",
                "responses": {
                    "200": {
                        "description": "Retorna o Token e o tipo do usuario",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "properties": {
                                        "id": {
                                            "type": "number",
                                            "example": 1
                                        },
                                        "name": {
                                            "type": "string",
                                            "example": "Empresa Tal"
                                        },
                                        "email": {
                                            "type": "string",
                                            "example": "fulano-de-tal@exemplo.com"
                                        },
                                        "email_verified_at": {
                                            "type": "string",
                                            "example": "2023-03-05T21:48:55.000000Z"
                                        },
                                        "type": {
                                            "type": "string",
                                            "example": "employer"
                                        },
                                        "created_at": {
                                            "type": "string",
                                            "example": "2023-03-05T21:48:55.000000Z"
                                        },
                                        "updated_at": {
                                            "type": "string",
                                            "example": "2023-03-05T21:48:55.000000Z"
                                        }
                                    },
                                    "type": "object"
                                }
                            }
                        }
                    },
                    "401": {
                        "description": "Retorno caso o token nao esteja registrado",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "properties": {
                                        "message": {
                                            "type": "string",
                                            "example": "Unauthenticated."
                                        }
                                    },
                                    "type": "object"
                                }
                            }
                        }
                    }
                },
                "security": [
                    {
                        "bearerToken": []
                    }
                ]
            }
        },
        "/api/auth/register": {
            "post": {
                "tags": [
                    "AuthController"
                ],
                "summary": "Registrar um novo colaborador",
                "description": "end-point utilizado para registrar um novo colaborador",
                "operationId": "4d6e95be84db27edf40c5f8bf6cc295a",
                "requestBody": {
                    "content": {
                        "application/json": {
                            "schema": {
                                "required": [
                                    "email",
                                    "password"
                                ],
                                "properties": {
                                    "cpf": {
                                        "type": "string",
                                        "example": "11222330188"
                                    },
                                    "name": {
                                        "type": "string",
                                        "example": "Fulano"
                                    },
                                    "surname": {
                                        "type": "string",
                                        "example": "De Tal"
                                    },
                                    "email": {
                                        "type": "string",
                                        "example": "teste_usuario@example.org"
                                    },
                                    "password": {
                                        "type": "string",
                                        "example": "1234"
                                    }
                                },
                                "type": "object"
                            }
                        }
                    }
                },
                "responses": {
                    "200": {
                        "description": "Retorna dados do usuario e dados do colaborador"
                    },
                    "500": {
                        "description": "Interna Server Error"
                    }
                }
            }
        },
        "/api/auth/employer/register": {
            "post": {
                "tags": [
                    "AuthController"
                ],
                "summary": "Registrar um novo empregador",
                "description": "end-point utilizado para registrar um novo empregador",
                "operationId": "7e2c0bfe50086b7ff22ca71b785428b5",
                "requestBody": {
                    "content": {
                        "application/json": {
                            "schema": {
                                "required": [
                                    "email",
                                    "password"
                                ],
                                "properties": {
                                    "cnpj": {
                                        "type": "string",
                                        "example": "11222333000188"
                                    },
                                    "bussiness_name": {
                                        "type": "string",
                                        "example": "Empresa Tal"
                                    },
                                    "fansasy_name": {
                                        "type": "string",
                                        "example": "Tal Empresa"
                                    },
                                    "name": {
                                        "type": "string",
                                        "example": "Tal Empresa"
                                    },
                                    "email": {
                                        "type": "string",
                                        "example": "teste_usuario@example.org"
                                    },
                                    "password": {
                                        "type": "string",
                                        "example": "1234"
                                    }
                                },
                                "type": "object"
                            }
                        }
                    }
                },
                "responses": {
                    "200": {
                        "description": "Retorna dados do usuario e dados do empregador"
                    },
                    "500": {
                        "description": "Interna Server Error"
                    }
                }
            }
        },
        "/api/employments": {
            "get": {
                "tags": [
                    "EmploymentController"
                ],
                "summary": "Obter todas as vagas cadastradas no sistema (Acesso publico para a area nao logada)",
                "description": "end-point os dados referentes as vagas do colaborador ou empregador logado utilizando o token de autorizacao",
                "operationId": "2b60611ddb8fc2fef3e5c1fabe4847ea",
                "responses": {
                    "200": {
                        "description": "Retorna um array de objetos com os dados referentes as vagas (Acesso publico)",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "type": "array",
                                    "items": {
                                        "properties": {
                                            "id": {
                                                "type": "number",
                                                "example": 1
                                            },
                                            "office": {
                                                "type": "string",
                                                "example": "Gerente"
                                            },
                                            "description": {
                                                "type": "string",
                                                "example": "Descricao da vaga como atribuicoes e responsabilidades"
                                            },
                                            "salary": {
                                                "type": "number",
                                                "example": 6000
                                            },
                                            "created_at": {
                                                "type": "string",
                                                "example": "2023-03-05T21:48:55.000000Z"
                                            },
                                            "updated_at": {
                                                "type": "string",
                                                "example": "2023-03-05T21:48:55.000000Z"
                                            }
                                        },
                                        "type": "object"
                                    }
                                }
                            }
                        }
                    },
                    "500": {
                        "description": "Internal Server Erro"
                    }
                }
            },
            "post": {
                "tags": [
                    "EmploymentController"
                ],
                "summary": "Registrar uma nova vaga",
                "description": "end-point utilizado para registrar uma nova vaga (apenas empregadores)",
                "operationId": "f2becf41be84d1bf31f1c203360dcea7",
                "requestBody": {
                    "content": {
                        "application/json": {
                            "schema": {
                                "required": [
                                    "email",
                                    "password"
                                ],
                                "properties": {
                                    "office": {
                                        "type": "string",
                                        "example": "Professor"
                                    },
                                    "description": {
                                        "type": "string",
                                        "example": "Descricao da vaga"
                                    },
                                    "salary": {
                                        "type": "number",
                                        "example": 1800.56
                                    }
                                },
                                "type": "object"
                            }
                        }
                    }
                },
                "responses": {
                    "204": {
                        "description": "No Content",
                        "headers": {
                            "Location": {
                                "description": "url para acessar a vaga",
                                "schema": {
                                    "type": "string"
                                }
                            }
                        }
                    },
                    "403": {
                        "description": "Forbiden (Ao tentar utilizar um token do tipo colaborador/employee)"
                    },
                    "500": {
                        "description": "Interna Server Error"
                    }
                },
                "security": [
                    {
                        "bearerToken": []
                    }
                ]
            }
        },
