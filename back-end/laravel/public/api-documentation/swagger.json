        "/api/employments/{id}": {
            "get": {
                "tags": [
                    "EmploymentController"
                ],
                "summary": "Obter as vaga especifica com base no id",
                "description": "end-point os dados referentes as vagas do colaborador ou empregador logado utilizando o token de autorizacao",
                "operationId": "ae2858fdcab329099c0d26f635c5d4ad",
                "parameters": [
                    {
                        "name": "id",
                        "in": "path",
                        "description": "id da vaga cadastrada",
                        "required": true,
                        "schema": {
                            "type": "integer"
                        }
                    }
                ],
                "responses": {
                    "200": {
                        "description": "Retorna os dados de uma vaga especifica",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "properties": {
                                        "id": {
                                            "type": "number",
                                            "example": 1
                                        },
                                        "office": {
                                            "type": "string",
                                            "example": "Gerente"
                                        },
                                        "description": {
                                            "type": "string",
                                            "example": "Descricao da vaga como atribuicoes e responsabilidades"
                                        },
                                        "salary": {
                                            "type": "number",
                                            "example": 6000
                                        },
                                        "created_at": {
                                            "type": "string",
                                            "example": "2023-03-05T21:48:55.000000Z"
                                        },
                                        "updated_at": {
                                            "type": "string",
                                            "example": "2023-03-05T21:48:55.000000Z"
                                        }
                                    },
                                    "type": "object"
                                }
                            }
                        }
                    },
                    "404": {
                        "description": "Not found"
                    },
                    "500": {
                        "description": "Internal Server Error"
                    }
                }
            },
            "put": {
                "tags": [
                    "EmploymentController"
                ],
                "summary": "Alterar uma vaga",
                "description": "end-point utilizado para alterar os dados da vaga (apenas empregadores)",
                "operationId": "5f0586004f60ee73dcd146d24624da3f",
                "parameters": [
                    {
                        "name": "id",
                        "in": "path",
                        "description": "id da vaga cadastrada",
                        "required": true,
                        "schema": {
                            "type": "integer"
                        }
                    }
                ],
                "requestBody": {
                    "content": {
                        "application/json": {
                            "schema": {
                                "required": [
                                    "email",
                                    "password"
                                ],
                                "properties": {
                                    "office": {
                                        "type": "string",
                                        "example": "Professor"
                                    },
                                    "description": {
                                        "type": "string",
                                        "example": "Descricao da vaga"
                                    },
                                    "salary": {
                                        "type": "number",
                                        "example": 1800.56
                                    }
                                },
                                "type": "object"
                            }
                        }
                    }
                },
                "responses": {
                    "204": {
                        "description": "No Content"
                    },
                    "403": {
                        "description": "Forbiden (Ao tentar utilizar um token do tipo colaborador/employee ou tentar alterar uma vaga que nao pertence a este usuario)"
                    },
                    "500": {
                        "description": "Interna Server Error"
                    }
                },
                "security": [
                    {
                        "bearerToken": []
                    }
                ]
            },
            "delete": {
                "tags": [
                    "EmploymentController"
                ],
                "summary": "Deletar uma vaga",
                "description": "end-point utilizado para deletar os dados da vaga (apenas empregadores)",
                "operationId": "cf4a81b00d973e04f8efb18fbe9b4fb8",
                "parameters": [
                    {
                        "name": "id",
                        "in": "path",
                        "description": "id da vaga",
                        "required": true,
                        "schema": {
                            "type": "integer"
                        }
                    }
                ],
                "responses": {
                    "204": {
                        "description": "No Content"
                    },
                    "403": {
                        "description": "Forbiden (Ao tentar utilizar um token do tipo colaborador/employee ou tentar deletar uma vaga que nao pertence a este usuario)"
                    },
                    "500": {
                        "description": "Interna Server Error"
                    }
                },
                "security": [
                    {
                        "bearerToken": []
                    }
                ]
            }
        }
    },
    "components": {
        "securitySchemes": {
            "bearerToken": {
                "type": "http",
                "description": "Baerer token obtido na autenticação",
                "name": "Authorization",
                "in": "header",
                "bearerFormat": "JWT",
                "scheme": "bearer"
            }
        }
    }
}